    "commit-msg": "node scripts/verifyCommitMsg.js"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@vuepress/plugin-pwa": "^1.5.3",
    "@vuepress/theme-vue": "^1.5.3",
    "axios": "^0.19.0",
